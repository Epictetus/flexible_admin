<%% models = %w() %>

<div class="topbar-wrapper" style="z-index: 5;">
  <div class="topbar" data-dropdown="dropdown" id="navigation">
    <div class="topbar-inner">
      <div class="container">
        <h3><a href="/admin">Home</a></h3>
        <ul class="nav">
          <%% models.each do |item| %>
            <%%= admin_nav_item(item) %>
          <%% end %>
        </ul>
        <div class="signin">
          <%% if current_admin_user.present? %>
            <%%= link_to "Sign out", destroy_admin_user_session_path %>
          <%% end %>
          
        </div><!-- .signin -->
      </div>
    </div><!-- /topbar-inner -->
  </div><!-- /topbar -->
</div><!-- /topbar-wrapper -->