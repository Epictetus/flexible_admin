<h2>Sign in</h2>
<%%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>

  <%%= render 'admin/shared/email_field', :f => f, :what => :email %>

  <%%= render 'admin/shared/password_field', :f => f, :what => :password %>

  <%% if devise_mapping.rememberable? -%>
    <div class="clearfix">
      <%%= f.label :remember_me %>
      <div class="input">
        <%%= f.check_box :remember_me %>
      </div>
    </div><!-- /clearfix -->
  <%% end -%>

  <div class="row">
    <%%= f.submit "Sign in", :class => 'btn primary medium right' %>
  </div>
<%% end %>

<%%= render "devise/shared/links" %>
